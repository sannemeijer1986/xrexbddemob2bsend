{
  "name": "xrexb2b",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:css": "mkdir -p public/css && sass src/scss/main.scss public/css/main.css --style=compressed --no-source-map",
    "watch:css": "sass --watch src/scss/main.scss:public/css/main.css --style=expanded --source-map",
    "watch:css-edit": "chokidar 'src/css/overrides.css' -c 'cp src/css/overrides.css src/scss/_overrides.scss'",
    "dev:with-css-edit": "npm-run-all -p watch:css-edit watch:css",
    "start": "npm run watch:css",
    "start:static": "npx --yes serve -s public -l 5173",
    "dev": "npm run watch:css",
    "dev:public": "npm-run-all -p watch:css start:static"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "chokidar-cli": "^3.0.0",
    "npm-run-all": "^4.1.5",
    "sass": "^1.93.3"
  },
  "dependencies": {
    "pdfkit": "^0.14.0"
  }
}
